main>\n\t\t\t<div class=\"search-box\">\n\t\t\t\t<input type=\"text\" name=\"input_city\" class=\"search-bar\" placeholder=\"Please enter the city name\" v-model=\"query\" @keypress=\"fetchWeather\"/>\n\t\t\t</div>\n\n\t\t\t<div class=\"weather-wrap\" v-if=\"typeof  weather.main != 'undefined'\">\n\t\t\t\t<div class=\"location-box\">\n\t\t\t\t\t<div class=\"info\">都市名 {{weather.name}}</div>\n\t\t\t\t\t<div class=\"info\">最高気温 {{Math.round(weather.main.temp_max)}}℃</div>\n\t\t\t\t\t<div class=\"info\">最低気温 {{Math.round(weather.main.temp_min)}}℃</div>\n\t\t\t\t\t<div class=\"info\">湿度 {{weather.main.humidity}}%</div>\n\t\t\t\t\t<div class=\"info\">天候 {{weather.weather[0].main}}</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<div class=\"weather-wrap\" v-if=\"typeof  weather.main == 'undefined' && flg == false\">\n\t\t\t\t<div class=\"location-box\">\n\t\t\t\t\t<div class=\"info\">入力した都市がありません</div>\n\t\t\t\t\t<div class=\"info\">再度入力してください</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</main>\n\t</div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default{\n\tname:'app',\n\tdata(){\n\t\treturn{\n\t\t\tquery: '',\n\t\t\tweather: {},\n\t\t\tflg: true,\n\t\t}\n\t},\n\tmethods: {\n\t\tfetchWeather(e){\n\t\t\tif(e.key == \"Enter\"){\n\t\t\t\tlet url = \"https://api.openweathermap.org/data/2.5/weather?q=\" + this.query + \"&units=metric&APPID=f0c99e31d9b8df5f12540f8e23d5e219\";\n\t\t\t\taxios.get(url)\n\t\t\t\t\t.then(res => {\n\t\t\t\t\t\tthis.flg = true;\n\t\t\t\t\t\tthis.weather = res.data;\n\t\t\t\t\t})\n\t\t\t\t\t.catch(error => {\n\t\t\t\t\t\tthis.flg = false;\n\t\t\t\t\t\tconsole.log(error);\n\t\t//\t\t\t\tthis.reset(error);\n\t\t\t\t\t});\n\t\t\t}\n\t\t},\n\t\treset(){\n\t\t\tthis.weather = 0;\n\t\t},\n\t}\n}\n</script>\n\n\n<style>\n\n\n\t#app{\n/*\t\tbackground-image: url('./assets/S__5627907.jpg');*/\n\t\tbackground-size:cover;\n\t\tbackground-position:bottom;\n\t\t\n\t}\n\n\t\n</style>\n"]}]}