ault{\n    data(){\n      return{\n        message:\"\",\n        image_list:{},\n      };\n    },\n    methods:{\n      search(e){\n        e.preventDefault();\n        let url = \"https://api.unsplash.com/search/photos?query=\"+this.message+\"&client_id=7TZKfiSUxqeHSnVU7F2OcPAOQYSFVOvm76GMSsPAQIk\";\n        axios.get(url)\n          .then(res => {\n              this.image_list = res.data.results;\n          });\n      },\n    },\n  }\n</script>\n\n\n<style>\n  .App{\n    text-align: center;\n  }\n</style>\n"]}]}